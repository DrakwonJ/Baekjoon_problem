#include <iostream>
#include <algorithm>
#include <stdio.h>
using namespace std;
int N,M,num_arr[9],visited[9];
void DFS(int cnt){
	if(cnt == M){
		for(int i=0;i<M;i++)
			cout<<num_arr[i]<<" ";
		cout<<"\n";
		return;
	}
	for(int i=1;i<=N;i++){
		num_arr[cnt] = i;
		DFS(cnt+1);
		}
}
int main(void){
	cin.tie(NULL);
	ios::sync_with_stdio(false);
	cin>>N>>M;
	for(int i=0;i<N;i++)
		num_arr[i] = i+1;
	DFS(0);
}
